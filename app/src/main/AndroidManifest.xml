<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.syndsmartqueue">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:testOnly="false">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".login" />
        <activity android:name=".otpconfirmation" />
        <activity android:name=".branches" />
        <activity
            android:name=".Dashboard"
            android:label="@string/title_activity_dashboard" />
        <activity android:name=".Scanner" />
        <activity android:name=".QueueRegister" />
        <activity android:name=".purposeofvisit" />
        <activity android:name=".withdrawform" />
        <activity android:name=".branchcleanessfeedback"></activity>
    </application>

</manifest>